<?xml version="1.0" encoding="UTF-8"?>
<deployments>
   <deployment>
      <instance>
         <instance-id>MessagePortletInstance</instance-id>
         <portlet-ref>koku-message-portlet</portlet-ref>         
         <portlet-preferences>
			<preference>
				<name>portletPath</name>
				<value>/portal/private/classic/Message</value>
			</preference>
			<preference>
				<name>refreshDuration</name>
				<value>60</value>
			</preference>
			<preference>
				<name>messageType</name>
				<value>2</value>
			</preference>
		</portlet-preferences>
      </instance>
   </deployment>
</deployments>
