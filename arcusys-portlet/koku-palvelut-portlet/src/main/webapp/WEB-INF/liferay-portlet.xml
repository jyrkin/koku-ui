<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 4.3.1//EN" "http://www.liferay.com/dtd/liferay-portlet-app_4_3_1.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>palvelut-portlet2</portlet-name>
		<icon>/images/icon.png</icon>
		<restore-current-view>false</restore-current-view>
		<private-session-attributes>false</private-session-attributes>
		<header-css>/css/Pager.css</header-css>
		<header-javascript>/js/localize-pager.js</header-javascript>
		<header-javascript>/js/jquery.pager.js</header-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>