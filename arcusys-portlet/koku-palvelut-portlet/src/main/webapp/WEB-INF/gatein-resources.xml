<?xml version="1.0" encoding="UTF-8"?>
<gatein-resources
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_0 http://www.gatein.org/xml/ns/gatein_resources_1_0"
    xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_0">
	
	<portlet-skin>
		<application-name>palvelut-portlet</application-name>
		<portlet-name>palvelut-portlet</portlet-name>
		<skin-name>SimpleSkin</skin-name>
		<css-path>/css/veera_portlet.css</css-path>
	</portlet-skin>
	
	<portlet-skin>
		<application-name>palvelut-portlet</application-name>
		<portlet-name>palvelut-portlet</portlet-name>
		<skin-name>Default</skin-name>
		<css-path>/css/veera_portlet.css</css-path>
	</portlet-skin>
	
	<javascript>
		<param>
			<js-module>jQuery</js-module>
			<js-path>/js/jquery-1.5.2.min.js</js-path>
			<js-priority>10</js-priority>
		</param>
		<param>
			<js-module>jQuery.pager</js-module>
			<js-path>/js/jquery.pager.js</js-path>
			<js-priority>10</js-priority>		
		</param>
		<param>
			<js-module>localize-pager</js-module>
			<js-path>/js/localize-pager.js</js-path>
			<js-priority>10</js-priority>		
		</param>
	</javascript>

</gatein-resources>

