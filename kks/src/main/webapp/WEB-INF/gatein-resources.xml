<?xml version="1.0" encoding="UTF-8"?>
<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_0 http://www.gatein.org/xml/ns/gatein_resources_1_0"
	xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_0">
	
	<portlet-skin>
		<application-name>kks-portlet-0.0.1-SNAPSHOT</application-name>
		<portlet-name>KKS</portlet-name>
		<skin-name>Default</skin-name>
		<css-path>/css/kks.css</css-path>
	</portlet-skin>

	<javascript>
		<param>
			<js-module>jQuery</js-module>
			<js-path>/js/jquery-1.5.2.min.js</js-path>
			<js-priority>10</js-priority>
		</param>
	</javascript>
  
    <javascript>
      <param>
        <js-module>jQueryIdle</js-module>
        <js-path>/js/jquery.idle-timer.js</js-path>
        <js-priority>10</js-priority>
      </param>
    </javascript>
    
    <javascript>
      <param>
        <js-module>jQueryUI</js-module>
        <js-path>/js/jquery-ui-1.8.20.custom.min.js</js-path>
        <js-priority>10</js-priority>
      </param>
    </javascript>
  
</gatein-resources>